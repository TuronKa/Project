{
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "esnext",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "cypress",
            "@faker-js/faker"
        ]
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ]
}